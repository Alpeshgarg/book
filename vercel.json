{
  "version": 2,
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/$1"
    },
    {
      "src": "/(api|contact|partner).*",
      "dest": "/api/index.js"
    },
    {
      "src": "/.*",
      "dest": "/api/index.js"
    }
  ]
}
