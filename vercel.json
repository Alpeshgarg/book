{
  "version": 2,
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|html))",
      "dest": "/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(contact|partner)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
